journal = {The Journal of the Acoustical Society of America},
number = {2},
pages = {503--517},
pmid = {15759672},
title = {{Equations for finite-difference, time-domain simulation of sound propagation in moving inhomogeneous media and numerical implementation.}},
volume = {117},
year = {2005}
}
