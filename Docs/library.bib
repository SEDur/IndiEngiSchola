doi = {10.1109/58.710587},
file = {:C$\backslash$:/Users/sedur/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Liu - 1998 - The pseudospectral time-domain (PSTD) algorithm for acoustic waves in absorptive media.pdf:pdf},
