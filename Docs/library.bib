keywords = {Acoustics,Atmosphere,Computer Simulation,Computer-Assisted,Fourier Analysis,Linear Models,Models,Motion,Numerical Analysis,Pressure,Reproducibility of Results,Sound,Surface Properties,Theoretical,Time Factors,Weather},
number = {4},
pages = {1632--1646},
pmid = {20968336},
title = {{The extended Fourier pseudospectral time-domain method for atmospheric sound propagation.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20968336{\%}7B{\%}25{\%}7D5Cnhttp://www.scopus.com/inward/record.url?eid=2-s2.0-78049478591{\%}7B{\&}{\%}7DpartnerID=tZOtx3y1},
