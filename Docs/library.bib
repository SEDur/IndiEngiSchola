@article{Lindau2012,
abstract = {The mixing time of room impulse responses denotes the moment when the diffuse re- verberation tail begins. A diffuse (“mixed”) sound field can physically be defined by (1) equidistribution of acoustical energy and (2) a uniform acoustical energy flux over the com- plete solid angle. Accordingly, the perceptual mixing time could be regarded as the moment when the diffuse tail cannot be distinguished from that of any other position or listener's orien- tation in the room. This, for instance, provides an opportunity for reducing the part of binaural room impulse responses that has to be updated dynamically inVirtual Acoustic Environments. Several authors proposed model- and signal-based estimators for the mixing time in rooms. Our study aims at an evaluation of all measures as predictors of a perceptual mixing time. Therefore, we collected binaural impulse response data sets with an adjustable head and torso simulator for a representative sample of rectangular shaped rooms. Altering the transition time into a homogeneous diffuse tail in real time in an adaptive, forced-choice listening test, we determined just audible perceptual mixing times.We evaluated the performance of all potential predictors by linear regression and finally obtained formulae to estimate the perceptual mixing time from measured impulse responses or physical properties of the room.},
author = {Lindau, Alexander and Kosanke, Linda and Weinzierl, Stefan},
file = {:C$\backslash$:/Users/sedur/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lindau, Kosanke, Weinzierl - 2012 - Perceptual Evaluation of Model- and Signal-Based Predictors of the Mixing Time in Binaural Room Impu.pdf:pdf},
isbn = {9781617387739},
journal = {Journal of the Audio Engineering Society},
title = {{Perceptual Evaluation of Model- and Signal-Based Predictors of the Mixing Time in Binaural Room Impulse Responses}},
volume = {60},
year = {2012}
}
@article{Moorer1978,
abstract = {The phase vocoder is an analysis-synthesis system that has as intermediate{\$}\backslash{\$}ndata the time-variant discrete Fourier spectrum of the input signal.{\$}\backslash{\$}nIt can be formulated in suchg a way that the synthesized signal is{\$}\backslash{\$}nidentical to the original, both theoretically and practically. The{\$}\backslash{\$}nintermediate data can be transformed, also with no loss of information,{\$}\backslash{\$}ninto the more conventional magnitude and frequency representation.{\$}\backslash{\$}nThese intermediate data can then be used to resynthesize the tone{\$}\backslash{\$}nat different pitches or different rates than the original with the{\$}\backslash{\$}nadvantage that when no modification is made, the synthetic tone is{\$}\backslash{\$}nabsolutely identical to the original. This represents a significant{\$}\backslash{\$}nadvance over the Heterodyne filter, which placed severe restrictions{\$}\backslash{\$}non the amount of variation in pitch or amplitude that could be analyzed.{\$}\backslash{\$}nThe phase vocoder has no such restrictions and can jsut as easily{\$}\backslash{\$}ndeal with vibrato and inharmonic tones. Since scaling the frequencies{\$}\backslash{\$}nin the phase vocoder analysis data also scales the spectrum up, use{\$}\backslash{\$}nof this modification with speech can produce altered vowel sounds.{\$}\backslash{\$}nIf this method is combined with the linear predictor, using the phase{\$}\backslash{\$}nvocoder to alter the pitch of the error signal, then the spectrum{\$}\backslash{\$}ncan be hald constant while the pitch is changed, thus allowing independent{\$}\backslash{\$}ncontrol over time, pitch, and spectrum. Vowel quality can be preserved{\$}\backslash{\$}nor altered at will. Again, if no modification is made, the combination{\$}\backslash{\$}nof the linear predictor and the phase vocoder is an identity, both{\$}\backslash{\$}ntheoretically and practically. This research is still in a very preliminary{\$}\backslash{\$}nstate, so only a few sound examples can be given at this time. A{\$}\backslash{\$}nfull theoretical explanation, however, can be given.},
author = {Moorer, James A},
doi = {Article},
issn = {00047554},
journal = {J. Audio Eng. Soc},
number = {1/2},
pages = {42--45},
title = {{The Use of the Phase Vocoder in Computer Music Applications}},
url = {http://www.aes.org/e-lib/browse.cfm?elib=3293},
volume = {26},
year = {1978}
}
