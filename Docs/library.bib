author = {Lee, P. and Vay, J. L.},
doi = {10.1016/j.cpc.2015.04.004},
file = {:E$\backslash$:/GIT/IndiEngiSchola/Docs/library/Efficiency of the perfectly matched layer with high-order finite difference and pseudospectral maxwell solvers.pdf:pdf},
